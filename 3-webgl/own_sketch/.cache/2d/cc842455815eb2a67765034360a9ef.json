{\n  console.log('PEAK')\n\ttexts.forEach((el, i) => {\n\t\tsetTimeout(() => {\n\t\t\tel.innerHTML = random(words)\n\t\t}, random(200) * i)\n\t})\n}\n\n\nfunction togglePlay(){\n  if(audio.isPlaying()){\n    audio.pause()\n  }else{\n    audio.loop()\n  }\n}"},"lineCount":null}},"error":null,"hash":"eba57e7564053192ee5d323bd0b726df","cacheData":{"env":{}}}